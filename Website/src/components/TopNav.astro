<script is:inline>
// Enable hidden nav bar
{
  const nav = document.querySelector(".main-top-nav");
  let lastScrollY = window.scrollY;

  window.addEventListener("scroll", () => {
    if (lastScrollY < window.scrollY) {
      nav.classList.add("nav--hidden");
    } else {
      nav.classList.remove("nav--hidden");
    }

    lastScrollY = window.scrollY;
  });
}

</script>

<script is:inline>
	// Clicking on the burst itself will click the checkbox. This gets rid of the reliance on clicking the input while keeping the hover animations 
	const toggleChecked = () => document.getElementById('Burst-Menu').toggleAttribute('checked');
document.getElementById('burst-toggle').addEventListener('click', toggleChecked);

	// Clicking on the menu will change the logo and burst fills to white
  const menuButton = document.getElementById('Black-Burst');
  const invertBackground = document.getElementById('black-path');    

  menuButton.addEventListener('click', () => {
    invertBackground.classList.toggle('Whitesmoke');
});

	// Clicking on the menu will change the logo and burst fills to white
  const whiteFill = document.getElementById('logo');    

  menuButton.addEventListener('click', () => {
    whiteFill.classList.toggle('Whitesmoke');
  });
</script>

