---
import { animate } from "motion"
import Blobity from "blobity";
---
<style>
	/* Input Label Container Styles */
	.Whitesmoke {fill: whitesmoke;}
	.white-logo {fill: whitesmoke;}
	input {display: none;}
	#Burst-Menu:checked ~ .container {clip-path: circle(100%);}
	input[type="checkbox"]:checked ~ .container {clip-path: circle(100%);}
    label {z-index: 5; width: 50px; height: 50px; border-radius: 50px; position: fixed; top: 64px; right: 200px; color: red; text-align: center; display: flex; align-items: center; justify-content: center; visibility: hidden;}

	/*Container Styles */
	    /* scoped to the component, other H1s on the page remain the same */
	.container { width: 100%; height: 100%; background: black; position: fixed; top: 0; right: 0; clip-path: circle(25px at 110% 8px); transition: all 500ms ease-in-out; display: flex; align-items: center; justify-content: center; z-index: 5;}
    .menu-left, .menu-right {display: flex; margin: 0 auto; width: 50%; justify-content: center;}
    .column-1, .column-2 {display: flex; flex-direction: column; margin: 2rem;}
	.column-3 {display: flex; flex-direction: column;}
    /* Menu Items Styles */
	.menu-subhead {display: flex; align-items: center;}
    .category-heading, .category-link {font-weight: 700; font-size: var(--step-4); margin-block: 2rem; color: whitesmoke;}
    .column-1 a, .column-2 a, .column-3 a {margin-block: 1rem; font-size: 2rem; color: whitesmoke;}
	.category-subhead {color: whitesmoke; margin: 0; font-size: var(--step-2); font-weight: 600;} .menu-subhead img {margin: 1rem;}
	.menu-right a h4 {margin:0 0 1rem 0;}

	    /* scoped to the component, other H1s on the page remain the same */
		#top-nav {}
	.main-top-nav {height: auto; margin-block: 2rem; width: 100%; z-index: 5; position: fixed; transition: all 500ms;}
    .navigation {padding: 0.5rem; display: flex; align-items: center; justify-content: space-between; width: 80%; margin: 0 auto;}
    .site-logo, .menu-burst {width: 100px; position: relative;}
    #Black-Burst {position: absolute; top: -40px; scale: 1.25; transition: all 0.5s;}
    #Red-Burst, #Green-Burst, #Blue-Burst {position: absolute; top: -40px; left: 0; bottom: 0; right: 0; mix-blend-mode: lighten; transition: all 0.5s;}
	.menu-burst {pointer-events: all;}
    .menu-burst:hover #Red-Burst {left: -32px; top: -56px; filter: blur(2px);}
    .menu-burst:hover #Green-Burst {right: -32px; top: -56px; filter: blur(2px);}
    .menu-burst:hover #Blue-Burst {top: -16px; filter: blur(2px);}
    .menu-burst:hover #Black-Burst {scale: 1.5;}
    #Blue-Burst {z-index: 1;} #Green-Burst {z-index: 2;} #Red-Burst {z-index: 3;} #Black-Burst {z-index: 10;}
	.menu-nav-divider {display: none;}

    /* Logo Scale Animation */
    .site-logo svg {transition: all 0.5s; mix-blend-mode: difference;}
    .site-logo:hover svg {scale: 1.125;}

	/* Hide the Navigation Bar on Scroll */
	section {--nav-height: 200px;}
	.nav--hidden {
  transform: translateY(calc(-1 * var(--nav-height)));
  box-shadow: none;
	}

	/* Change Logo and Burst Color on Menu Click */
	#black-path {transition: all 0.5s;}

	/* Dark Mode Styles */
	body.dark-mode #black-path {fill: whitesmoke;}
    body.dark-mode .Black {fill: whitesmoke;}
	body.dark-mode .site-logo svg {fill: whitesmoke;}

/* MOBILE */
@media screen and (max-width: 600px) {
	.site-logo, .menu-burst {width: 85px; position: relative;}
	.container {flex-direction: column; overflow: scroll;}
	.menu-left, .menu-right {flex-direction: column; width: 100%;}
	.column-1, .column-3 {margin: 0 2rem;}
	.menu-left a {margin-top: 1.5rem; margin-bottom: 0; font-size: var(--step-3);}
	.menu-left h4 {margin-top: 2rem; margin-bottom: 1rem; font-size: var(--step-4)}
	.column-3 h4 {font-size: var(--step-3);}
	.column-3 a h4 {font-size: var(--step-2);} .column-3 a h4:first-child {font-size: var(--step-1);}
	.column-3 a {margin-top: 0;}
	.menu-nav-divider {display: block; padding: 5rem; margin: 5rem;}
}
  </style>

